# MOVE OUTSIDE OF propulsion BEFORE MERGING

<?xml version="1.0"?>
<package format="2">
  <name>propulsion</name>
  <version>0.1.0</version>
  <description>
	  Package for moving the AUV. Designed to abstract away the mechanical 
	  implementation details of the propulsion mechanism.
  </description>
	  
  <maintainer email="dev@mcgillrobotics.com">McGill Robotics</maintainer>
  <license>GPLv3</license>
  <url type="repository">https://github.com/mcgill-robotics/AUV-2025</url>

  <buildtool_depend>colcon</buildtool_depend>

  <buildtool_depend>rosidl_default_generators</buildtool_depend>

  <exec_depend>rosidl_default_runtime</exec_depend>

  <!-- message dependencies -->
  <member_of_group>rosidl_interface_packages</member_of_group>
  <depend>auv_msgs</depend>
  <depend>geometry_msgs</depend>
  <depend>python3-numpy</depend>
  <depend>rospy</depend>

  <!-- rclcpp dependencies -->
  <buildtool_depend>ament_cmake_ros</buildtool_depend>
  <depend>rclcpp</depend>
  <export>
    <build_type>ament_cmake</build_type>
    </export>

  <!-- rclpy dependencies -->
  <depend>rclpy</depend>
  <export>
    <build_type>ament_python</build_type>
    </export>

</package>

