<launch>
  <!-- Set this to your camera's name -->
  <arg name="cam_name" value="down_cam" />

  <!-- Start the GSCAM node -->
  <env name="GSCAM_CONFIG" value="nvarguscamerasrc sensor-id=0 ! video/x-raw(memory:NVMM),width=4032,height=3040,framerate=30/1 ! nvvidconv ! video/x-raw,format=YUV9,width=1920,height=1080,framerate=30/1 ! nvvidconv ! videoconvert ! ffmpegcolorspace " />
  <node pkg="gscam" type="gscam" name="$(arg cam_name)">
    <param name="camera_name" value="$(arg cam_name)" />
    <remap from="camera/image_raw" to="$(arg cam_name)/image_raw" />
  </node>


</launch>

<!-- gst-launch-1.0  -->
