<?xml version="1.0"?>
<package format="2">
  <name>state_estimation</name>
  <version>0.1.0</version>
  <description>
	  Aggregates states from various sensors and publishes poses at regular interval.
  </description>

  <maintainer email="dev@mcgillrobotics.com">McGill Robotics</maintainer>
  <license>GPLv3</license>
  <url type="respository">https://github.com/mcgill-robotics/AUV-2020</url>

  <buildtool_depend>catkin</buildtool_depend>

  <build_depend>depth_sensor</build_depend>
  <build_depend>auv_msgs</build_depend>
  <build_depend>imu</build_depend>
  <build_depend>geometry_msgs</build_depend>
  <build_depend>rospy</build_depend>
  <build_depend>std_msgs</build_depend>

  <exec_depend>depth_sensor</exec_depend>
  <exec_depend>imu</exec_depend>
  <exec_depend>geometry_msgs</exec_depend>
  <exec_depend>auv_msgs</exec_depend>
  <exec_depend>rospy</exec_depend>
  <exec_depend>std_msgs</exec_depend>
</package>
